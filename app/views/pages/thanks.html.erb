        <script type="text/javascript">
        var prg_width = 2400;
 
        function progress() {
            var node = document.getElementById('progress');
            var w    = node.style.width.match(/\d+/);
 
            if (w == prg_width) {
                w = 0;
            }
 
            node.style.width = parseInt(w) + 5 + 'px';
        }
 
        </script>



